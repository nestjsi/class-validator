  "dependencies": {
    "class-validator": "^0.13.*",
    "validator": "^13.6.*"
  },
  "description": "Additions for class validator",
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@r37r0m0d3l/rollup-config-pkg": "git+https://github.com/r37r0m0d3l/rollup-config-pkg.git",
    "@types/validator": "^13.7.1",
    "dts-generator": "3.0.0",
    "prettier": "2.3.1",
    "rollup-plugin-typescript2": "0.29.0",
    "typescript": "4.3.4"
  },
  "exports": {
    ".": {
      "default": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "node": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
      },
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/index.cjs",
    "dist/index.cjs.map",
    "dist/index.d.ts",
    "dist/index.js",
    "dist/index.js.map",
    "dist/index.mjs",
    "dist/index.mjs.map"
  ],
  "homepage": "https://github.com/nestjsi/class-validator#readme",
  "keywords": [
    "decorators",
    "typescript",
    "validation",
    "validator"
  ],
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "name": "@nestjsi/class-validator",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nestjsi/class-validator.git"
  },
  "scripts": {
    "build": "rollup --config rollup.config.mjs",
    "build:definitions": "dts-generator --project . --out ./dist/index.d.ts",
    "package:update": "npx npm-check --update"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.1.4"
}
